{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NEXTAUTH_URL": {
      "required": true
    },
    "NEXTAUTH_SECRET": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    }
  },
  "functions": {
    "api/**": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
